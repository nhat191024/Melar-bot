# Environment Variables Example
# Copy this file to .env and fill in your values

# Discord Bot Configuration (required)
DISCORD_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_GUILD_ID=your_guild_id_for_testing

# MySQL Database Configuration (required)
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=discord_bot

# Bot Settings
BOT_PREFIX=!
NODE_ENV=development

# Developer Configuration (Your Discord User ID for dev commands)
DEVELOPER_ID=your_discord_user_id_here

# Module Configuration (set to 'false' to disable)
MODULE_FUN=true
MODULE_AUTOFORUMPOST=true
MODULE_XFIXER=true
